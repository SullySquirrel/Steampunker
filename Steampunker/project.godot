run/size/window_height_override=180
run/size/window_width_override=320
run/stretch/mode="disabled"

[autoload]

AudioManager="*res://audio_manager.tscn"
ControllerIcons="*res://addons/controller_icons/ControllerIcons.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
Global="*res://Scripts/Global.gd"

